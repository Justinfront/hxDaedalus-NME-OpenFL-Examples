<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="bitmapPathfinding04" package="hxDaedalus.examples.bitmapPathfinding04.bitmapPathfinding04" version="1.0.0" company="JLM" />
	
	<!-- output -->
	<app main="hxDaedalus.examples.bitmapPathfinding04.Main" file="bitmapPathfinding04" path="bin" />
	
	<window background="#ffffff" fps="60" />
	<window width="1024" height="780" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="hxDaedalus" />
	
	<!-- assets -->
	<icon path="assets/openfl.svg" />
	<assets path="assets/img" rename="img" />
	
	<!-- optimize output
	<haxeflag name="-dce full" /> -->
	
</project>
<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="BitmapPathfinding" package="lib.daedalus.hx" version="1.0.0" company="daedalus" />
	<app main="BitmapPathfinding" path="bin" file="BitmapPathfinding" />
	<window background="#FFFFFF" fps="60" />
	<window width="1024" height="780" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<set name="SWF_VERSION" value="11.2" />
	
	<source path="src" />
	<source path="../../src" />
	<haxeflag name="-D" value="swf-script-timeout=60" />
	<!--haxeflag name="--no-traces" if="release" /-->
	<haxelib name="openfl" />
	
	<assets path="assets/galapagosBW.png" rename="GalapagosBW" if="html5" />
	<assets path="assets/galapagosColor.png" rename="GalapagosColor" if="html5" />
</project>